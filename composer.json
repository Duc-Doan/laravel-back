{
    "name": "laravel/back",
    "description": "Laravel Backend design pattern.",
    "keywords": ["laravel", "backend", "request", "response", "service"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/back/issues",
        "source": "https://github.com/laravel/back"
    },
    "authors": [
        {
            "name": "Duc Doan",
            "email": "ducdoanhoang@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^6.9|^7.0|^8.0",
        "illuminate/database": "^6.9|^7.0|^8.0",
        "illuminate/support": "^6.9|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Back\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Back\\LaravelBackServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
